from sklearn.linear_model import LinearRegression

# Read input values
num_observed_features, num_rows = map(int, input().split())

# Initialize lists to store observed features and prices
X_observed = []
y_observed = []

# Read observed data
for _ in range(num_rows):
    data = list(map(float, input().split()))
    X_observed.append(data[:-1])  # Features (excluding the last element)
    y_observed.append(data[-1])   # Price per square foot

# Create and fit a linear regression model
model = LinearRegression()
model.fit(X_observed, y_observed)

# Read the number of houses for which pricing is not known
num_unknown = int(input())

# Initialize list to store features of houses for which pricing is not known
X_unknown = []

# Read features of houses for which pricing is not known
for _ in range(num_unknown):
    data_unknown = list(map(float, input().split()))
    X_unknown.append(data_unknown)

# Predict prices for houses with unknown pricing
y_predicted = model.predict(X_unknown)

# Print the predicted prices
for price in y_predicted:
    print("%.2f" % price)
